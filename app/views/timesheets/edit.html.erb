<h1>Editing timesheet</h1>

<%= render "form", timesheet: @timesheet %>

<br>

<div>
  <%= link_to "Show this timesheet", @timesheet %> |
  <%= link_to "Back to timesheets", timesheets_path %>
</div>


<div class="container mt-3">
  <div class="row">
    <div class="col-9">
      <h3 class="mt-3">Editar</h3>
    </div>
    <div class="col-3 text-end">
    </div>
    <%= render "form", timesheet: @timesheet %>
    <br>
    <div>
      <%= link_to "Mostrar", @timesheet, class: "btn btn-primary" %>
      <%= link_to "Voltar", timesheets_path, class: "btn btn-primary" %>
    </div>
  </div>
</div>

